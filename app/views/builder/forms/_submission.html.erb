<article class="form-submission">
  <%= content_tag(
    :h4, 
    link_to(
      submission.title, 
      builder_site_form_submission_path(
        current_site, current_form, submission
      )
    ),
    :class => 'title'
  ) %>
  <%= content_tag(
    :time, 
    "#{time_ago_in_words(submission.updated_at)} ago", 
    :class => 'date'
  ) %>
</article>
